elems = ['item 1', 'item 2', 'item 3']
for(10, i)
    # the * symbol unpacks a list
    now = i.cycle(*elems)
    print('index ', i, ': ', now)
end
# index 0: item 1
# index 1: item 2
# index 2: item 3
# index 3: item 1
# index 4: item 2
# index 5: item 3
# index 6: item 1
# index 7: item 2
# index 8: item 3
# index 9: item 1