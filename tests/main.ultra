include @Core.Request

$url = 'https://reqres.in/api/'
req = new Request($url)
req.headers = {
    'Content-Type': 'application/json',
    'Accept': 'application/json'
}
req.timeout = 15
req.body = JSON.create(
    {
        'name': 'Alan Telles',
        'username': 'alantelles'
    }
)
resp = req.run(:post, 'users')

resp.headers.each(function(key, value, _key)
    print(key,': ', value)
end)

print('Return: ----\n', resp.text)