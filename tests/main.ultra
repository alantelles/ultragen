include @Core.ByteStream

class StringStream
function init(body) : StringStream
    self.parent = new ByteStream(body)
end
function save(name) : StringStream
    print("Estou salvando um arquivo")
    self.parent.save(name)
end 

m = new StringStream("talking for my bullshit")
m.save('ctuhulh.txt')
