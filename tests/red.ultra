decorator wrap(fn, other)
    print('in wrap')
    print(other)
    fn(a, b)
    print('out wrap')
end

decorator rewrap(fn, fn2)
    print('in rewrap')
    fn()
    fn2()
    print('out rewrap')
end

function a()
    print('a')
end
function b()
    print('b')
end

k = rewrap(function()
    print('passando anony')
end,b)
k()