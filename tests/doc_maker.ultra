function getPath(type, member)
    include @Core.OS
    home = '/home/alantelles/develop/lazarus/ultragen/examples/docs_examples/'
    path = home + type + '/' + member + '.ultra'
    return path
end

include @Core.FileSystem
include @Core.Request

type = $params[0]
member = $params[1]
id = $params[2]
path = getPath(type, member)
content = FileSystem.loadText(path)
# content.prepend('```ruby')
# content.append('```')

body = {"examples": '\\n'.join(content).replace('"', '\\"')}
print(JSON.create(body))
r = Request.put('http://localhost:5000/api/members/' + id + '/', JSON.create(body), {"Content-Type": "application/json"})
print(r.text)