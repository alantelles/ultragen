function searchArtist()
    headers = setHeaders($ENV[:keys][:search])
    
    query = $request[:query][:q]
    url = "https://genius.p.rapidapi.com/search?q=" + query
    r = Request.get(url, headers)
    if ((r.status) == 200)
        return JSON.parse(r.text)
    else
        return null
    end
